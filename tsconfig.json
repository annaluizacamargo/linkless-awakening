{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "esModuleInterop": true,
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@assets/*": ["src/assets/*"],
      "@components/*": ["src/components/*"],
      "@hooks/*": ["src/hooks/*"],
      "@store/*": ["src/store/*"],
      "@styles/*": ["src/styles/*"],
      "@types/*": ["src/types/*"],
      "@pages/*": ["src/pages/*"]
    }
  }
}
